@using RadioStore.BusinessAccessLayer.ModelsDTO
@model IEnumerable<ProductSpecificationDTO>
@{
    ViewBag.Title = "EditProductSpecifications";
}

<h4>Edit prices for @ViewBag.ProductName</h4>

@using (Html.BeginForm("EditProductSpecifications", "Product", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.Hidden("productId", new { productId = ViewBag.ProductId })
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.EditorFor(model => Model)
    <div>
        <input name="command" type="submit" value="Update types from category" class="btn btn-outline-primary" />
    </div>
    <div>
        <input name="command" type="submit" value="Submit" class="btn btn-outline-success" />
    </div>
}



